		{
			"character": "character-1642268344.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "Oh haha the snoop must be me. I’m Joey, I was passing by this town and had to check it out."
		},
		{
			"choice": "Snoop?!?",
			"condition": "",
			"definition": "",
